/* _kmButtons - kmButtons based on "Super Awesome kmButtons With CSS3 and RGBA": http://www.zurb.com/article/266/super-kmButton-kmButtons-with-css3-and-rgba 
==========================================*/
/* Set the primary color for the kmButtons; */
.kmButton{
  background-color: #e1025d !important;
  color: #fff !important;
}

.kmButton.grey{
  background-color: #111 !important;
  color: #fff !important;
}

.kmButton.delete{
  background-color: #800 !important;
  color: #fff !important;
}

.kmButton.complete{
  background-color: #017F34 !important;
  color: #fff !important;
}

.kmButton.disabled{ 
  cursor: default !important;
}

.kmButton.full{
  width: 73%;
}

/* the kmButton size gets set here. Feel free to add new sizes, like .kmButton.small or .small.large   */
.kmButton{
  padding: 9px 40px 10px !important; 
  font-size: 13px !important;
}

.kmButton.disabled,
.kmButton:active{ padding: 10px 40px 9px !important; }

.kmButton.registration{ 
  padding: 7px 4px 7px !important; 
  font-size: 13px !important;
}

.kmButton.registration:active{ padding: 8px 4px 6px !important; }

.kmButton.xsmall{ 
  padding: 3px !important; 
  font-size: 11px !important;
}

.kmButton.xsmall:active{ padding: 4px 3px 2px !important; }

.kmButton.small{ 
  padding: 4px 4px 4px !important; 
  font-size: 13px !important;
}

.kmButton.small:active{ padding: 5px 4px 3px !important; }

.kmButton.large{ 
  padding: 9px 40px 10px !important; 
  font-size: 13px !important;
}

.kmButton.large:active{ padding: 10px 40px 9px !important; }

/* Touch the rest at your own risk. */
.kmButton{ 
  border: 0 !important; 
  cursor: pointer !important; 
  font-family: inherit !important;
  font-style: normal !important; 
  font-weight: bold !important; 
  line-height: 1 !important; 
  position: relative !important; 
  text-align: center !important;
  text-decoration: none !important; 
  outline: none !important;
  
  /* Important to maintain vertical margin */
  margin-top: 0 !important;
  margin-bottom: 0 !important;
  
  /* not all browsers support these, but who cares? */
  text-shadow: 0 -1px 1px rgba(0,0,0,0.25), -2px 0 1px rgba(0,0,0,0.25) !important; 
  
  border-radius: 5px !important; 
  -moz-border-radius: 5px !important; 
  -webkit-border-radius: 5px !important; 
  box-shadow: 0 1px 2px rgba(0,0,0,0.5) !important; 
  -moz-box-shadow: 0 1px 2px rgba(0,0,0,0.5) !important; 
  -webkit-box-shadow: 0 1px 2px rgba(0,0,0,0.5) !important;
  
  /* who needs images these days? */
  background-image: -moz-linear-gradient(top, rgba(255,255,255,.2), rgba(150,150,150,.2), rgba(0,0,0,.0)) !important;
  background-image: -webkit-gradient(linear, 0% 0%, 0% 100%, from(rgba(255,255,255,.2)), color-stop(0.5, rgba(150,150,150,.2)), to(rgba(0,0,0,.0))) !important;
  filter: progid:DXImageTransform.Microsoft.gradient(startColorstr=#30ffffff,endColorstr=#10000000);
  
  /* cross-browser inline block hack 
     see http://blog.mozilla.com/webdev/2009/02/20/cross-browser-inline-block/ */
  display: -moz-inline-stack;
  display: inline-block;
  vertical-align: middle !important;
  *display: inline !important;
  position: relative;
  
  /* IE luv */
  zoom: 1;
  
  /*disable text selection (Firefox only)*/
  -moz-user-select: none;
  
  /* no text selection for iOS */
  -webkit-tap-highlight-color: rgba(0,0,0,0);
  -webkit-user-select: none;
  -webkit-touch-callout: none;
}

/* OPERA only
   if there is no border set, Opera shows a transparent background-color if border-radius is set. */
@media all and (-webkit-min-device-pixel-ratio:10000),not all and (-webkit-min-device-pixel-ratio:0) {
  input.kmButton {
    border: 1px solid RGBa(0,0,0,0) !important;
  }
}

/* hide selection background color */
.kmButton::selection{
  background: transparent;
}

.kmButton{
  outline: 0; /*remove dotted line, works for links only*/
}

.kmButton::-moz-focus-inner{
  border: none; /* remove dotted lines for kmButtons */
}

.kmButton:focus,
.kmButton:hover{
  background-image: -moz-linear-gradient(top, rgba(255,255,255,.4), rgba(150,150,150,.3), rgba(0,0,0,.0)) !important;
  background-image: -webkit-gradient(linear, 0% 0%, 0% 100%, from(rgba(255,255,255,.4)), color-stop(0.5, rgba(150,150,150,.3)), to(rgba(0,0,0,.0))) !important;
  #filter: progid:DXImageTransform.Microsoft.gradient(startColorstr=#50ffffff,endColorstr=#20000000);
}

.kmButton.disabled,
.kmButton:active.disabled,
.kmButton:active{ 
  background-image: -moz-linear-gradient(top, rgba(0,0,0,.2), rgba(150,150,150,.2), rgba(255,255,255,.2)) !important;
  background-image: -webkit-gradient(linear, 0% 0%, 0% 100%, from(rgba(0,0,0,.2)), color-stop(0.5, rgba(150,150,150,.2)), to(rgba(255,255,255,.2))) !important;
  filter: progid:DXImageTransform.Microsoft.gradient(startColorstr=#20000000,endColorstr=#50ffffff);
  box-shadow: inset 0 1px 2px rgba(0,0,0,0.7) !important;
  -moz-box-shadow: inset 0 1px 2px rgba(0,0,0,0.7) !important; 
  -webkit-box-shadow: inset 0 1px 2px rgba(0,0,0,0.7) !important;
}

/* Safari doesn't support inset box shadow, so we'd better remove it */
@media screen and (-webkit-min-device-pixel-ratio:0){
  .kmButton.disabled,
  .kmButton:active	{ 
    -webkit-box-shadow: none;
  }
}

